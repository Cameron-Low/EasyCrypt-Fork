(* -------------------------------------------------------------------- *)
type 'a t. type t1. type t2.

(* -------------------------------------------------------------------- *)
op (+) : 'a t -> 'a t -> 'a t.

op (++) (x y :'a t) = x + x + y.

op (+++) (x:'a t) y = x + x ++ y.

(* -------------------------------------------------------------------- *) 
op a, b, c : 'a t.

op e : t1 t * t2 t = (a<:t1>, b<:t2>).
