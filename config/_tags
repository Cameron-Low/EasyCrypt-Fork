# --------------------------------------------------------------------
true : use_menhir, menhir_explain
true : debug
true : warn_Z, warn_Y, warn_+28

# --------------------------------------------------------------------
<src> or <pprint> : include

# --------------------------------------------------------------------
<src/*.{ml,mli}> : package(menhirLib), package(why3), package(extunix)
<src/*.{native,byte}> : package(menhirLib), package(why3), package(extunix)
